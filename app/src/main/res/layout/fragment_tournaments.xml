<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_horizontal_margin"
    tools:context=".TeamprofileActivity">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/titulo_contacto"
                android:layout_width="match_parent"
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:text="Contacto"/>
            <TextView
                android:id="@+id/celular_contacto"
                android:layout_below="@+id/titulo_contacto"
                android:layout_width="wrap_content"
                android:padding="10dp"
                android:layout_alignParentLeft="true"
                android:layout_height="wrap_content"
                android:text="Celular"/>
            <TextView
                android:id="@+id/txt_celular_contacto"
                android:layout_below="@+id/titulo_contacto"
                android:layout_width="wrap_content"
                android:layout_toRightOf="@+id/celular_contacto"
                android:padding="10dp"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content"
                android:text="Celular"/>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
</RelativeLayout>